# mount point	fstype		device
/efs			ext4		/dev/block/mmcblk0p3
/boot			emmc		/dev/block/mmcblk0p8
/recovery		emmc		/dev/block/mmcblk0p9
/cache			ext4		/dev/block/mmcblk0p14
/system			ext4		/.secondrom/media/.secondrom/system.img
/data			ext4		/.secondrom/media/.secondrom/data 	fstype2=bind
/modem			emmc		/dev/block/mmcblk0p10

/sdcard			datamedia   /dev/null
/external_sd	vfat		/dev/block/mmcblk1p1 fstype2=auto
